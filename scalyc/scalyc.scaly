if args.length() < 1 {
    return(null)
}
else {
    let options: Options = Options.parseArguments(args)
        catch OptionsError.invalidOption(error: invalidOption) return(null)
        catch OptionsError.noOutputOption(error: noOutputOption) return(null)
        catch OptionsError.noFilesToCompile(error: noFilesToCompile) return(null)

    Compiler.compileFiles(options) catch _ (error: OptionsError) return(null)
}

null
