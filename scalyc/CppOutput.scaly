class CppModule {

    let name: String
    let header: String
    let source: String
    
    constructor() {
        this.name = null
        this.header = null
        this.source = null
    }
    
    constructor(name: String, header: String, source: String) {
        this.name = name
        this.header = header
        this.source = source
    }
}

class CppProgram {

    constructor() {
        name = null
        project = null
        main = null
        modules = null
    }

    constructor(name: String, project: String, main: String, modules: CppModule[]) {
        this.name = name
        this.project = project
        this.main = main
        this.modules = modules
    }

    let name: String
    let project: String
    let main: String
    let modules: CppModule[]
}
